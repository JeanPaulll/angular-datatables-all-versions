{
  "name": "angular-datatables-all-versions",
  "version": "1.2.2",
  "license": "MIT",
  "private": false,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test -sm=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:docs": "compodoc ./libs -p src/tsconfig.packages.json -s",
    "build:appTables": "cpr dist/angular-datatables-all-versions src/angular-datatables-all-versions --overwrite && ng build --app packages && rimraf src/angular-datatables-all-versions",
    "build:app": "cpr dist/angular-datatables-all-versions src/angular-datatables-all-versions --overwrite && ng build --app packages && rimraf src/angular-datatables-all-versions",
    "build:app:prod": "cpr dist/angular-datatables-all-versions src/angular-datatables-all-versions --overwrite && ng build --prod --named-chunks=true --app packages && rimraf src/angular-datatables-all-versions",
    "build:libs:datatable": "ng-packagr -p libs/datatable/package.json",
    "publish:libs:datatable": "npm publish dist/angular-datatables-all-versions"
  },
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/cdk": "^7.3.1",
    "@angular/common": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/core": "^7.2.4",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.4",
    "@angular/http": "^7.2.4",
    "@angular/material": "^7.3.1",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/router": "^7.2.4",
    "bootstrap": "^4.0.0",
    "core-js": "^2.6.3",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "lodash-es": "^4.17.4",
    "ngx-md": "^7.1.3",
    "ngx-popup-dialog": "^1.0.8",
    "node-sass": "^4.12.0",
    "popper.js": "^1.14.3",
    "rxjs": "^6.4.0",
    "tslint-angular": "^1.1.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular-devkit/core": "7.3.0",
    "@angular-devkit/schematics": "7.3.0",
    "@angular/cli": "7.3.0",
    "@angular/compiler-cli": "^7.2.4",
    "@angular/language-service": "^7.2.4",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^10.12.21",
    "codelyzer": "^4.5.0",
    "cpr": "^3.0.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.7.1",
    "protractor": "^5.4.2",
    "rimraf": "^2.6.1",
    "rxjs-tslint": "^0.1.6",
    "ts-node": "~4.1.0",
    "tsickle": "^0.34.3",
    "tslint": "^5.12.1",
    "typescript": "3.2.4"
  }
}
